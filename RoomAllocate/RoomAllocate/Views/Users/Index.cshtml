@model IEnumerable<RoomAllocate.Models.Users>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MindName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrackName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MindName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrackName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MNO }) |
